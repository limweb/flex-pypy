PREFIX ltw : <http://www.lt-world.org/ltw.owl#>
PREFIX owl : <http://www.w3.org/2002/07/owl#>
PREFIX rdf : <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?definition
              WHERE {
                                ?term_obj rdf:type ltw:Semantic_Web .
                                ?term_obj ltw:definition ?definition .
                              
			}
        